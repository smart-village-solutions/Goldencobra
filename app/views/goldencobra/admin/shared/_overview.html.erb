<% order_by ||= "id" %>
<div class="overview-sidebar">
	<ul>
		<% overview.order(order_by.to_sym).each do |root| %>
			<%= render :partial => "/goldencobra/admin/shared/item", :object => root, :locals => {:link_name => link_name, :url_path => url_path, :order_by => order_by } %>
		<% end %>
	</ul>
</div>